@model CqrsSample.Models.ChangePasswordModel

@using(Html.BeginForm("ChangePassword","Users"))
{
    @Html.HiddenFor(x=> x.UserId)
    @Html.LabelFor(x=> x.OldPassword)
    @Html.TextBoxFor(x=> x.OldPassword)
    @Html.LabelFor(x=> x.NewPassword)
    @Html.TextBoxFor(x => x.NewPassword)
    <input type="submit"/>
}